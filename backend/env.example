# Google Cloud Configuration
GOOGLE_APPLICATION_CREDENTIALS=goog_cred.json
GOOGLE_CLOUD_PROJECT=your-project-id

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG_MODE=true

# Security
SECRET_KEY=your-secret-key-here-change-this-in-production
ALLOWED_ORIGINS=http://localhost:8080,http://127.0.0.1:8080

# Optional: Google Cloud API Keys (if not using service account)
# GOOGLE_CLOUD_VISION_API_KEY=your-vision-api-key
# GOOGLE_CLOUD_DOCUMENT_AI_API_KEY=your-document-ai-api-key
# GOOGLE_GENERATIVE_AI_API_KEY=your-generative-ai-api-key

# Optional: Logging
LOG_LEVEL=INFO
LOG_FILE=app.log

# Optional: File Upload Settings
MAX_FILE_SIZE=10485760  # 10MB in bytes
UPLOAD_DIR=uploads
